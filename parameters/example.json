{
    "env": {
        "n_agents": 5,
        "x_dim": 25,
        "y_dim": 25,
        "n_cities": 2,
        "max_rails_between_cities": 2,
        "max_rails_in_city": 3,
        "seed": 42
    },
    "obs": {
        "class": "TreeObs",
        "tree_depth": 2,
        "radius": 10
    },
    "exp": {
        "start": 1.0,
        "end": 0.01,
        "decay": 0.997
    },
    "trn": {
        "memory_size": 100000,
        "batch_size": 32,
        "update_every": 8,
        "learning_rate": 0.5e-4,
        "tau": 1e-3,
        "gamma": 0.99,
        "buffer_min_size": 0,
        "hidden_sizes": [128],
        "use_gpu": true
    },
    "pol": {
        "class": "DQNAgent",
        "checkpoint_freq": 100,
        "base_dir": ""
    }
}