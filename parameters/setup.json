{
    "sys": {
        "seed": 42,
        "base_dir": "",
        "obs_class": "TreeObs",
        "agent_class": "DDDQNAgent",
        "log_class": "WandBLogger",
        "memory_class": "ReplayBuffer"
    },
    "obs": {
        "tree_depth": 2,
        "radius": 10
    },
    "trn": {
        "memory_size": 100000,
        "batch_size": 32,
        "update_every": 8,
        "learning_rate": 0.5e-4,
        "tau": 1e-3,
        "gamma": 0.99,
        "buffer_min_size": 0,
        "hidden_sizes": [128],
        "exp_start": 1.0,
        "exp_end": 0.01,
        "exp_decay": 0.997,
        "checkpoint_freq": 100,
        "target_update": 10000,
        "soft_update": true,
        "env": "t1.l0"
    },
    "log": {
        "log_dir": "tmp/logs",
        "hp_dir": "hparam_tuning",
        "attributes": {
            "completions": ["avg"], 
            "scores": ["avg"], 
            "steps": ["avg"], 
            "loss": ["avg"], 
            "deadlocks": ["avg"], 
            "exploration_prob": ["val"], 
            "min_steps": [], 
            "exploration_count": ["val"], 
            "act_0": ["avg"], 
            "act_1": ["avg"], 
            "act_2": ["avg"], 
            "act_3": ["avg"], 
            "act_4": ["avg"],
            "time_act": ["avg"],
            "time_train": ["avg"]
        },
        "hp_params_filename": "parameters/hp.json"
    }
}